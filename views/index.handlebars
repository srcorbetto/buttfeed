<div class="container">
	<div class="row">
	{{#each fromDB}}
			<div class="col-md-6">
				<div class="card mt-3 mb-3">
				  <div class="card-body">
				    <a href="{{this.link}}"><h3 class="title">{{this.title}}</h3></a>
				    <p class="summary">{{this.summary}}</p>
				    <a href="{{this.link}}"><img src="{{this.img}}" alt="" class="img"></a>
				    <i class="fa fa-heart" aria-hidden="true" data-id="{{this._id}}"></i>
				  </div>
				</div>
			</div>
	{{/each}}
	</div>
</div>